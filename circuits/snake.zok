import "./mimc7" as mimc7;

def main(private field poisonX, private field poisonY, field snakeState, field poisonHash) -> bool {

    field poisonIndex = poisonX * 5 + poisonY;
    field poisonState = 1 << poisonIndex;

    assert(mimc7::<25>(poisonState, 0) == poisonHash);

    bool isSnakeHit = (poisonState & snakeState) > 0;

    return (isSnakeHit);
}
